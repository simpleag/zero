<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model">
     <class name="Choice" table="choice">
        <id name="choId" column="choId" >
              <generator class="uuid"/>
        </id>
        <property name="claId" type="string" column="claId"  />
        <property name="useId" type="string" column="useId"  />
     </class>
</hibernate-mapping>


<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model">
     <class name="Subject" table="subject">
        <id name="subId" column="subId" >
              <generator class="uuid"/>
        </id>
        <property name="subName" type="string" column="subName"  />
        <property name="subTeaId" type="string" column="subTeaId"  />
        <property name="subInfo" type="string" column="subInfo"  />
        <property name="subTab" type="string" column="subTab"  />
     </class>
</hibernate-mapping>

